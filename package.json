{
  "name": "nestjs-redis-example",
  "version": "0.0.1",
  "description": "A simple NestJS Redis example",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/index",
    "start:dev": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "prettier --write \"src/**/*.ts\" && eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --coverage",
    "test:dev": "jest --watch"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/websockets": "^9.4.1",
    "@prisma/client": "^4.14.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^5.3.2",
    "parse-duration": "^1.0.3",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "i": "^0.3.7",
    "jest": "29.5.0",
    "npm": "^9.6.6",
    "prettier": "^2.3.2",
    "prisma": "^4.14.0",
    "sinon": "^15.0.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.(t|j)s",
      ".args.(t|j)s",
      "src/types"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
